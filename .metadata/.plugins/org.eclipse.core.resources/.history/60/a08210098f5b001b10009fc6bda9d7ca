package day0121;
import java.util.*;
public class Ex02For2 {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        //1. 1부터 10까지 출력
        for(int i=1; i<=10; i++) {
            System.out.print(i+" ");
            
        }
        System.out.println();
        
        //2. 8부터 3까지 출력
        for(int i=8; i>=3; i--) {
            System.out.print(i+" ");
        }System.out.println();
        
        //3. 사용자로부터 시작숫자와 끝숫자를 받아서 차례대로 출력
        int start,end;
        System.out.println("시작숫자 : ");
        start = sc.nextInt();
        System.out.println("끝숫자 : ");
        end = sc.nextInt();
        if(start>end) {
            for(;start==end;start--) {
                System.out.print(start+" ");
            }System.out.println();
        }
        else
        {
            for(;start==end;start++) {
                System.out.print(start + " ");
            }System.out.println();
        }
        
        //4. 숫자하나 입력받아서 1부터 홀수만 출력
        int num;
        System.out.println("숫자를 입력하세요 : ");
        num = sc.nextInt();
        
        for(int i=1; i<=num; i=i+2) {
            System.out.print(i+" ");
        }
        
        //5. 숫자 하나 입력받고 그 수까지의 합을 출력
        int num5=0;
        System.out.println("숫자를 입력하세요 : ");
        num5 : sc.nextInt();
        for(int i=1, result=0; i<=num5; i++) {
            result+=i;
            System.out.print(result + " ");
        }
        
        //6. 하나 입력받고 1부터 그 수 까지의 곱을 출력
        int num6=0;
        System.out.println("숫자를 입력하세요 : ");
        num6 : sc.nextInt();
        for(int i=1, result =1; i<=num6; i++) {
            result*=i;
            System.out.print(result + " ");
        }
    }
}
